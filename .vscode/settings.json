{
  "cSpell.words": [
    "abortable",
    "activitybar",
    "bangle.dev",
    "collab",
    "comms",
    "INDEXDB",
    "inputrules",
    "kushanjoshi",
    "outdent",
    "textblock",
    "tr's"
  ],
  "peacock.color": "#f28ea1",
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEventsOnParentDirectory",
    "watchDirectory": "useFsEvents"
  },
  "npm.packageManager": "yarn",
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#f7bcc7",
    "activityBar.activeBorder": "#288e11",
    "activityBar.background": "#f7bcc7",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#288e11",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#f28ea1",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#ed607b",
    "titleBar.activeBackground": "#f28ea1",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#f28ea199",
    "titleBar.inactiveForeground": "#15202b99",
    "sash.hoverBorder": "#f7bcc7",
    "statusBarItem.remoteBackground": "#f28ea1",
    "statusBarItem.remoteForeground": "#15202b"
  }
}
